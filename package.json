{
  "name": "diplom",
  "version": "1.0.0",
  "description": "automation test for module test, api test, e2e test",
  "main": "index.js",
  "scripts": {
    "unit:test": "jest --config='./unit/config/jest.config.js'",
    "unit:report": "start ./unit/jest-html-report/report.html",
    "api:test": "jest --config='./api/config/jest.config.js'",
    "api:report": "start ./api/jest-html-report/report.html",
    "ui:test": "playwright test --config=./ui/config/playwright.config.ts",
    "ui:report": "playwright show-report ui/assets/reporter",
    "lint": "eslint */tests/**/*.ts",
    "lint:fix": "eslint */tests/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SniazhanaMarkevich/diplom.git"
  },
  "author": "Sniazhana_Markevich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SniazhanaMarkevich/diplom/issues"
  },
  "homepage": "https://github.com/SniazhanaMarkevich/diplom#readme",
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.2",
    "@types/superagent": "^4.1.24",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^42.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-expect-message": "^1.1.3",
    "jest-html-reporter": "^3.10.2",
    "jest-jasmine2": "^29.7.0",
    "prettier": "^3.1.0",
    "superagent": "^8.1.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  }
}
